{% for links in content.field_social_media_links %}
    {% for link in links['#paragraph'].field_social_media_network %}
        <p><a class="{{ link.value|lower }}" href="{{ links['#paragraph'].field_social_media_url.uri }}" target="_blank">{{ link.value|replace({"Tumbler":"Tumblr"}) }}</a></p>
    {% endfor %}
{% endfor %}


