{
  "name": "pantheon-systems/example-drops-8-composer",
  "description": "Install drops-8 with Composer on Pantheon.",
  "type": "project",
  "license": "MIT",
  "extra": {
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"],
      "web/libraries/ckeditor/plugins/{$name}": ["vendor:ckeditor-plugin"],
      "web/libraries/{$name}": ["type:drupal-library"]
    },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor-plugin/embed",
        "version": "4.11.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/embed/releases/embed_4.11.1.zip",
          "type": "zip"
        }
      }
    }
  ],
    "require": {
    "ckeditor-plugin/embed": "4.11.1",
    "composer/installers": "^1.0.20",
    "cweagans/composer-patches": "^1.0",
    "dmore/behat-chrome-extension": "^1.2",
    "drupal-composer/drupal-scaffold": "^2.0.1",
    "drupal/adminimal_theme": "^1.3",
    "drupal/better_exposed_filters": "^3.0@alpha",
    "drupal/block_type_templates": "^1.0@alpha",
    "drupal/block_visibility_groups": "^1.2",
    "drupal/bootstrap": "^3.10",
    "drupal/bootstrap_barrio": "^4.8",
    "drupal/bootstrap_library": "^1.9",
    "drupal/bootstrap_paragraphs": "^2.0@beta",
    "drupal/cas_attributes": "1.x-dev",
    "drupal/chosen": "^2.5",
    "drupal/ckeditor_media_embed": "^1.3",
    "drupal/components": "^1.0",
    "drupal/config_direct_save": "^1.0",
    "drupal/config_filter": "^1.2",
    "drupal/config_ignore": "^2.1",
    "drupal/config_import_de": "^1.0@alpha",
    "drupal/config_installer": "^1.0",
    "drupal/config_split": "^1.3",
    "drupal/console": "^1.0",
    "drupal/core": "^8",
    "drupal/easy_breadcrumb": "^1.8",
    "drupal/easy_install": "^10.0",
    "drupal/facets": "^1.0@beta",
    "drupal/flag": "^4.0@alpha",
    "drupal/group": "^1.0@RC",
    "drupal/imce": "^1.6",
    "drupal/inline_entity_form": "^1.0@beta",
    "drupal/libraries": "3.x-dev",
    "drupal/link_attributes": "^1.3",
    "drupal/linkit": "^4.3",
    "drupal/migrate_plus": "4.x-dev",
    "drupal/migrate_source_csv": "^2.0",
    "drupal/migrate_tools": "4.x-dev",
    "drupal/migrate_upgrade": "^3.0@rc",
    "drupal/paragraphs": "^1.2",
    "drupal/pathauto": "^1.0",
    "drupal/redirect": "^1.2",
    "drupal/redirect_after_login": "^2.2",
    "drupal/redis": "^1.0",
    "drupal/scheduler": "^1.0",
    "drupal/search_api_db": "^1.7",
    "drupal/search_api_pantheon": "~1.0",
    "drupal/sendgrid_integration": "^1.2",
    "drupal/simple_block": "^1.0@beta",
    "drupal/simple_sitemap": "^2.12",
    "drupal/stage_file_proxy": "^1.0@alpha",
    "drupal/twig_field_value": "^1.1",
    "drupal/twig_tweak": "^2.0",
    "drupal/views_field_view": "^1.0@beta",
    "drupal/webform": "5.0-rc22",
    "drupal/webform_migrate": "^1.0@alpha",
    "drupal/weight": "^3.1",
    "drush/drush": "~8",
    "rvtraveller/qs-composer-installer": "^1.1",
    "tightenco/collect": "^5.6",
    "webflo/drupal-core-strict": "^8"
  },
  "require-dev": {
    "behat/behat": "3.*",
    "behat/mink": "^1.7",
    "behat/mink-extension": "^2.2",
    "behat/mink-goutte-driver": "^1.2",
    "bex/behat-screenshot": "^1.2",
    "consolidation/robo": "^1.3",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.3",
    "drupal/coder": "^8.2.12",
    "drupal/devel": "^1.0",
    "drupal/drupal-extension": "^3.1",
    "drush-ops/behat-drush-endpoint": "^0.0.4",
    "jcalderonzumba/gastonjs": "^1.0.2",
    "jcalderonzumba/mink-phantomjs-driver": "^0.3.1",
    "mikey179/vfsstream": "^1.2",
    "pantheon-systems/quicksilver-pushback": "~1",
    "phpunit/phpunit": "^4.8",
    "symfony/css-selector": "^2.8"
  },
  "conflict": {
      "drupal/drupal": "*",
      "doctrine/common":"2.8"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "scripts": {
    "build-assets": [
      "@prepare-for-pantheon",
      "composer install --optimize-autoloader"
    ],
    "drupal-unit-tests": "cd web/core && ../../vendor/bin/phpunit --testsuite=unit --exclude-group Composer,DependencyInjection,PageCache",
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "lint": "robo lint",
    "unit-test": "robo unit",
    "prepare-for-pantheon": "DrupalProject\\composer\\ScriptHandler::prepareForPantheon",
    "post-install-cmd": [
      "@drupal-scaffold",
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-create-project-cmd": [
      "@drupal-scaffold",
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ]
  },
    "build-env": {
      "install-cms": [
        "drush site-install standard --account-mail={account-mail} --account-name={account-name} --account-pass={account-pass} --site-mail={site-mail} --site-name={site-name} --yes",
        "drush pm-enable config_direct_save simple_block --yes",
        "drush pm-uninstall block_content --yes"
      ],
      "export-configuration": "drush config-export --yes"
    },
    "drupal-scaffold": {
      "source": "https://raw.githubusercontent.com/pantheon-systems/drops-8/{version}/{path}",
      "includes": [
        "sites/default/default.services.pantheon.preproduction.yml",
        "sites/default/settings.pantheon.php"
      ],
      "excludes": [
        ".csslintrc",
        ".editorconfig",
        ".eslintignore",
        ".eslintrc.json",
        ".htaccess",
        "web.config",
        "robots.txt"
      ]
    },
    "patches-file": "composer.patches.json"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
