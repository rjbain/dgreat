{
  "name": "pantheon-systems/example-drops-8-composer",
  "description": "Install drops-8 with Composer on Pantheon.",
  "type": "project",
  "license": "MIT",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
        "name": "ckeditor-plugin/embed",
        "version": "4.11.1",
        "type": "ckeditor-plugin",
        "dist": {
          "url": "https://download.ckeditor.com/embed/releases/embed_4.11.1.zip",
          "type": "zip"
        }
      }
    }
  ],
    "require": {
    "ckeditor-plugin/embed": "4.11.1",
    "composer/installers": "^1.0.21",
    "consolidation/annotated-command": "^2.12.0",
    "cweagans/composer-patches": "^1.0",
    "dmore/behat-chrome-extension": "^1.2",
    "drupal/address": "^1.4",
    "drupal/adminimal_theme": "^1.3",
    "drupal/better_exposed_filters": "5.0@beta",
    "drupal/block_type_templates": "^1.0@alpha",
    "drupal/block_visibility_groups": "^1.2",
    "drupal/bootstrap": "^3.10",
    "drupal/bootstrap_barrio": "^5.5",
    "drupal/bootstrap_library": "^2.0",
    "drupal/bootstrap_paragraphs": "^2.0@beta",
    "drupal/cas": "^1.5",
    "drupal/cas_attributes": "^2.0@beta",
    "drupal/chosen": "^2.5",
    "drupal/ckeditor_media_embed": "^1.3",
    "drupal/components": "^3.0@beta",
    "drupal/config_direct_save": "^2.1",
    "drupal/config_filter": "^1.9",
    "drupal/config_ignore": "^2.1",
    "drupal/config_split": "^1.8",
    "drupal/console": "^1.0",
    "drupal/core": "^9",
    "drupal/core-composer-scaffold": "^9",
    "drupal/core-project-message": "^9",
    "drupal/core-recommended": "^9",
    "drupal/delete_all": "^2.0",
    "drupal/devel": "^4.1",
    "drupal/easy_breadcrumb": "^2.0",
    "drupal/easy_install": "^10.0",
    "drupal/facets": "^2.0",
    "drupal/file_delete": "^1.0",
    "drupal/flag": "^4.0@alpha",
    "drupal/group": "^1.3",
    "drupal/imce": "^2.4",
    "drupal/inline_entity_form": "^1.0@RC",
    "drupal/jquery_ui_checkboxradio": "^1.2",
    "drupal/jquery_ui_datepicker": "^1.2",
    "drupal/libraries": "3.x-dev",
    "drupal/link_attributes": "^1.3",
    "drupal/linkit": "^6.0@beta",
    "drupal/migrate_plus": "^5.1",
    "drupal/migrate_source_csv": "^3.4",
    "drupal/migrate_tools": "4.x-dev",
    "drupal/migrate_upgrade": "^3.0@rc",
    "drupal/module_missing_message_fixer": "^2.0",
    "drupal/pantheon_advanced_page_cache": "^1.0",
    "drupal/paragraphs": "^1.6",
    "drupal/pathauto": "^1.0",
    "drupal/redirect": "^1.2",
    "drupal/redirect_after_login": "^2.3",
    "drupal/redis": "^1.0",
    "drupal/scheduler": "^1.0",
    "drupal/search_api": "^1.23",
    "drupal/search_api_db": "^1.7",
    "drupal/search_api_pantheon": "^3.0",
    "drupal/search_api_solr": "^4.2",
    "drupal/seckit": "^2.0",
    "drupal/sendgrid_integration": "^1.2",
    "drupal/simple_block": "^1.0@beta",
    "drupal/sitewide_alert": "^1.3",
    "drupal/stage_file_proxy": "^1.0@alpha",
    "drupal/taxonomy_import": "^2.0",
    "drupal/twig_field_value": "^2.0",
    "drupal/twig_tweak": "^2.0",
    "drupal/upgrade_rector": "^1.0@alpha",
    "drupal/upgrade_status": "^3.13",
    "drupal/views_field_view": "^1.0@beta",
    "drupal/viewsreference": "^2.0@beta",
    "drupal/webform": "^6.1",
    "drupal/webform_migrate": "^1.0@alpha",
    "drupal/webform_views": "^5.0@alpha",
    "drupal/weight": "^3.1",
    "drupal/workbench": "^1.1",
    "drupal/workbench_access": "^1.0",
    "drupal/xmlsitemap": "^1.0",
    "drush/drush": "^10",
    "foxy/foxy": "^1.1.2",
    "pantheon-systems/drupal-integrations": "^9",
    "pantheon-systems/quicksilver-pushback": "^2",
    "rvtraveller/qs-composer-installer": "^1.1",
    "tightenco/collect": "^5.6",
    "zaporylie/composer-drupal-optimizations": "^1.0"
  },
  "require-dev": {
    "behat/behat": "^3",
    "behat/mink": "^1.7",
    "behat/mink-extension": "^2.2",
    "behat/mink-goutte-driver": "^1.2",
    "bex/behat-screenshot": "^1.2",
    "consolidation/robo": "^1.3",
    "drupal/coder": "^8.2.12",
    "drupal/core-dev": "^9",
    "drupal/drupal-extension": "^4",
    "drush-ops/behat-drush-endpoint": "^0.0.4",
    "jcalderonzumba/gastonjs": "^1.0.2",
    "jcalderonzumba/mink-phantomjs-driver": "^0.3.1",
    "kint-php/kint": "^4.1",
    "mikey179/vfsstream": "^1.2",
    "phpspec/prophecy-phpunit": "^2",
    "phpstan/phpstan": "^1.4",
    "phpunit/phpunit": "^9"
  },
  "conflict": {
      "drupal/drupal": "*",
      "doctrine/common":"2.8"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "scripts": {
    "build-assets": [
      "@prepare-for-pantheon",
      "composer install --optimize-autoloader"
    ],
    "drupal-unit-tests": "cd web/core && ../../vendor/bin/phpunit --testsuite=unit --exclude-group Composer,DependencyInjection,PageCache",
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "lint": "robo lint",
    "unit-test": "robo unit",
    "prepare-for-pantheon": "DrupalProject\\composer\\ScriptHandler::prepareForPantheon",
    "post-install-cmd": [
      "@drupal-scaffold",
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-create-project-cmd": [
      "@drupal-scaffold",
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ]
  },
  "extra": {
    "installer-types": ["ckeditor-plugin"],
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"],
      "web/libraries/ckeditor/plugins/{$name}": ["type:ckeditor-plugin"],
      "web/libraries/{$name}": ["type:drupal-library"]
    },
    "build-env": {
      "install-cms": [
        "drush site-install standard --account-mail={account-mail} --account-name={account-name} --account-pass={account-pass} --site-mail={site-mail} --site-name={site-name} --yes",
        "drush pm-enable config_direct_save simple_block --yes",
        "drush pm-uninstall block_content --yes"
      ],
      "export-configuration": "drush config-export --yes"
    },
    "drupal-scaffold": {
      "locations": {
        "web-root": "./web"
      },
      "file-mapping": {
        "[web-root]/robots.txt": false,
        "[web-root]/.gitignore": false,
        "[web-root]/sites/default/default.settings.php": false,
        "[web-root]/sites/development.services.yml": false,
        "[web-root]/sites/example.settings.local.php": false,
        ".csslintrc": false,
        ".editorconfig": false,
        ".eslintignore": false,
        ".eslintrc.json": false,
        ".htaccess": false,
        "web.config": false,
        "robots.txt": false
      },
      "allowed-packages": [
        "pantheon-systems/drupal-integrations"
      ],
      "source": "https://raw.githubusercontent.com/pantheon-systems/drops-8/{version}/{path}",
      "includes": [
        "sites/default/default.services.pantheon.preproduction.yml",
        "sites/default/settings.pantheon.php"
      ],
      "excludes": [
        ".csslintrc",
        ".editorconfig",
        ".eslintignore",
        ".eslintrc.json",
        ".htaccess",
        "web.config",
        "robots.txt"
      ]
    },
    "patches-file": "composer.patches.json",
    "enable-patching": "true"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "cweagans/composer-patches": true,
      "drupal/console-extend-plugin": true,
      "drupal/core-composer-scaffold": true,
      "rvtraveller/qs-composer-installer": true,
      "zaporylie/composer-drupal-optimizations": true,
      "drupal/core-project-message": true,
      "foxy/foxy": true,
      "simplesamlphp/composer-module-installer": true,
      "joachim-n/composer-manifest": true,
      "oomphinc/composer-installers-extender": true
    }
  }
}
